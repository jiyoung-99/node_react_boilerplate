{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended", "airbnb"],
    "rules": {
        "prettier/prettier": ["error"]
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": 0,
        "no-console": 0,
        "arrow-parens": 0
    }
}
